{
    "name": "oilstone/template-api",
    "type": "project",
    "description": "Online Solutions Ltd. API project template",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "garethhudson07/api": "^3.22",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "oilstone/api-resource-loader": "^1.4",
        "oilstone/logging": "^2.0",
        "oilstone/system-status": "^1.0.0",
        "spatie/laravel-responsecache": "^6.6"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "nunomaduro/collision": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
